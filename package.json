{
  "name": "ely-cli",
  "version": "1.0.3",
  "description": "CLI tool for Elysia.js projects",
  "type": "module",
  "bin": {
    "ely-cli": "./dist/cli.js"
  },
  "main": "./dist/cli.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/cli.ts --outdir ./dist --target node",
    "start": "node dist/cli.js",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "elysia",
    "cli",
    "generator",
    "bun",
    "elysiajs",
    "scaffold",
    "boilerplate"
  ],
  "author": "Alex Veros <alexveros46@mail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jorlex27/ely-cli.git"
  },
  "homepage": "https://github.com/Jorlex27/ely-cli#readme",
  "engines": {
    "node": ">=16.0.0"
  },
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^9.2.10",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.7",
    "typescript": "^5.2.2"
  }
}
